							</owl:ObjectProperty>
						</owl:inverseOf>
					</owl:ObjectProperty>
				</rdf:predicate>
				<rdf:object>
					<rdf:Description rdf:nodeID="version" />
				</rdf:object>
			</rdf:Statement>
